<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.flashlight_first">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_flashlight1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FlashLight_First">
        <activity android:name=".TrafficActivity"
            android:label="Traffic Light"
            android:parentActivityName=".MenuActivity"></activity>
        <activity
            android:name=".PoliceLightActivity"
            android:label="Police"
            android:parentActivityName=".MenuActivity"/>
        <activity
            android:name=".WarringActivity"
            android:label="Warring Light"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".NeonActivity"
            android:label="Neon Light"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".LightActivity"
            android:label="Light"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".MenuActivity"
            android:label="Menu FlashLight" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>